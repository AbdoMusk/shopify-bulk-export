{
  "name": "shopify-data-export",
  "type": "module",
  "version": "1.0.0",
  "description": "A package to help you bulk export data from Shopify's API.",
  "license": "MIT",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup --dts-resolve",
    "test": "vitest run",
    "prepublishOnly": "pnpm run build",
    "lint": "eslint . --fix"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "consola": "^3.2.3",
    "got": "^12.6.1",
    "graphql": "^16.8.1",
    "p-wait-for": "^5.0.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.4.6",
    "@types/node": "^20.10.5",
    "eslint": "^8.56.0",
    "tsup": "8.0.1",
    "typescript": "5.3.3",
    "vitest": "1.1.0"
  }
}
